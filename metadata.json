{
  "name": "juame-ospuppet",
  "version": "0.2.1",
  "author": "juame",
  "summary": "Puppet module for Puppet (Puppet Server)",
  "license": "Apache-2.0",
  "source": "https://github.com/juame/juame-ospuppet.git",
  "project_page": "https://github.com/juame/juame-ospuppet",
  "issues_url": "https://github.com/juame/juame-ospuppet/issues",
